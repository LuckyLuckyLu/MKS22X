public class KnightBoard{
    private int[][] board;
    public KnightBoard(int row, int col){
	board = new int[row][col];
	for (int i = 0; i < row; i++){
	    for (int j = 0; j < col; i++){
		board[i][j] = 0;    
		}
	}	     
    }

